<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".LocationEntryActivity">

    <Button
        android:id="@+id/backButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="80dp"
        android:text="Back to Main"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayout"
        app:layout_constraintVertical_bias="0.0" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout"
        android:layout_width="387dp"
        android:layout_height="392dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/houseTextView"
            android:layout_width="176dp"
            android:layout_height="42dp"
            android:text="Your House"
            android:textSize="20sp"
            tools:layout_editor_absoluteX="216dp"
            tools:layout_editor_absoluteY="31dp" />

        <EditText
            android:id="@+id/friendLatEditText"
            android:layout_width="186dp"
            android:layout_height="45dp"
            android:ems="10"
            android:inputType="textPersonName"
            android:text="Latitude"
            tools:layout_editor_absoluteX="18dp"
            tools:layout_editor_absoluteY="323dp" />

        <EditText
            android:id="@+id/houseLabelEditText"
            android:layout_width="173dp"
            android:layout_height="45dp"
            android:ems="10"
            android:inputType="textPersonName"
            android:text="Label"
            tools:layout_editor_absoluteX="216dp"
            tools:layout_editor_absoluteY="85dp" />

        <EditText
            android:id="@+id/friendLabelEditText"
            android:layout_width="178dp"
            android:layout_height="43dp"
            android:ems="10"
            android:inputType="textPersonName"
            android:text="Label"
            tools:layout_editor_absoluteX="217dp"
            tools:layout_editor_absoluteY="323dp" />

        <EditText
            android:id="@+id/friendLongEditText"
            android:layout_width="186dp"
            android:layout_height="44dp"
            android:ems="10"
            android:inputType="textPersonName"
            android:text="Longitude"
            tools:layout_editor_absoluteX="18dp"
            tools:layout_editor_absoluteY="267dp" />

        <TextView
            android:id="@+id/familyTextView"
            android:layout_width="176dp"
            android:layout_height="42dp"
            android:text="Family's House"
            android:textSize="20sp"
            tools:layout_editor_absoluteX="216dp"
            tools:layout_editor_absoluteY="146dp" />

        <TextView
            android:id="@+id/friendTextView"
            android:layout_width="176dp"
            android:layout_height="42dp"
            android:text="Friend's House"
            android:textSize="20sp"
            tools:layout_editor_absoluteX="216dp"
            tools:layout_editor_absoluteY="269dp" />

        <EditText
            android:id="@+id/houseLatEditText"
            android:layout_width="186dp"
            android:layout_height="45dp"
            android:ems="10"
            android:inputType="textPersonName"
            android:text="Latitude"
            tools:layout_editor_absoluteX="18dp"
            tools:layout_editor_absoluteY="88dp" />

        <EditText
            android:id="@+id/houseLongEditText"
            android:layout_width="185dp"
            android:layout_height="44dp"
            android:ems="10"
            android:inputType="textPersonName"
            android:text="Longitude"
            tools:layout_editor_absoluteX="18dp"
            tools:layout_editor_absoluteY="31dp" />

        <EditText
            android:id="@+id/familyLabelEditText"
            android:layout_width="178dp"
            android:layout_height="43dp"
            android:ems="10"
            android:inputType="textPersonName"
            android:text="Label"
            tools:layout_editor_absoluteX="217dp"
            tools:layout_editor_absoluteY="204dp" />

        <EditText
            android:id="@+id/familyLatEditText"
            android:layout_width="186dp"
            android:layout_height="45dp"
            android:ems="10"
            android:inputType="textPersonName"
            android:text="Latitude"
            tools:layout_editor_absoluteX="18dp"
            tools:layout_editor_absoluteY="201dp" />

        <EditText
            android:id="@+id/familyLongEditText"
            android:layout_width="186dp"
            android:layout_height="44dp"
            android:ems="10"
            android:inputType="textPersonName"
            android:text="Longitude"
            tools:layout_editor_absoluteX="18dp"
            tools:layout_editor_absoluteY="149dp" />
    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>